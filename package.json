{
  "name": "react-redux",
  "version": "1.0.0",
  "description": "Personio Demo",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --config configs/webpack.dev.js",
    "build:dev": "webpack --config configs/webpack.dev.js -d",
    "build:prod": "webpack --config configs/webpack.prod.js && cp -r ./public/* ./dist",
    "utest": "jest",
    "utest:watch": "jest --watch",
    "utest:update-snapshot": "jest --updateSnapshot",
    "test": "yarn lint:test && yarn utest",
    "lint:test": "eslint \"src/**/*.{ts,tsx}\" --quiet",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --quiet --fix"
  },
  "author": "senthilc",
  "license": "",
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.25.0",
    "bootstrap": "5.1.0",
    "bootstrap-icons": "^1.7.0",
    "classnames": "^2.3.1",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.6",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.177",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.18",
    "@types/redux": "^3.6.0",
    "@types/redux-saga": "^0.10.5",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "bootstrap-loader": "^3.0.0",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-webpack-plugin": "^3.0.1",
    "expose-loader": "^0.7.5",
    "file-loader": "^6.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.3.2",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.8.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.2",
    "prop-types": "15.6.2",
    "react-test-renderer": "^17.0.2",
    "resolve-url-loader": "^2.3.0",
    "sass": "^1.38.1",
    "sass-loader": "^12.1.0",
    "sass-resources-loader": "^1.3.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^3.2.1",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^3.0.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "typings": "^2.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.svg$": "./configs/svgTransform.js"
    },
    "testEnvironment": "jest-environment-jsdom",
    "testRegex": "(tests/.*|(\\.|/))(spec).(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "./"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy",
      "^react(.*)$": "<rootDir>/node_modules/react$1"
    }
  }
}
